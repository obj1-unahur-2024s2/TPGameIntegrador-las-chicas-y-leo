import wollok.game.*
import niveles.*

// este script es el INICIAL, donde podremos ejecutar el videojuego

program ghostCoffee {

	game.title("Ghost Coffee")

	game.width(18)
	game.height(12)
	game.cellSize(64)
	// resolucion total = 1152 x 768

	game.boardGround("escenarioPrueba3.png") // fondo de testeo

	nivel1.iniciar() // esta línea da inicio al videojuego, encuentran al objeto y el método en archivo "niveles"

	game.start()
}
